{
    "name": "omegaalfa/cookie",
    "description": "Class for operations with cookies in PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "omegaalfa\\Cookie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "omegaalfa\\Cookie\\tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "another-vendor/package": "v1.*"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
